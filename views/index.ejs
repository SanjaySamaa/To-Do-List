    <%-include("header")-%>
    <div class="box" id="heading">
        <h1><%= listTitle %></h1>
    </div>
    <div class="box">   
 
        <% items.forEach(element=>{%>
          <div class="item">  
            <input type="checkbox">
            <p><%= element %> </p>
        </div>
        <%});%>
    
    
    <form action="/" method="post" class="item">
        <input autocomplete="off" type="text" name="newItem" placeholder="New Item" autofocus required>
        <button type="submit" name="list" value=<%= listTitle %>>+</button>
    </form>
    <%-include("footer")-%>    
